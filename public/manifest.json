{
  "manifest_version": 2,
  "name": "Chrome Extension",
  "description": "Chrmo Extension for simple navigation of social media platforms",
  "version": "0.0.1",
  "author": "Kalyan Kumar",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": ["activeTab"]
}